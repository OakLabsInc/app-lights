{
  "name": "lights-application",
  "version": "0.0.1",
  "description": "Oak Lights app for OakOS v5.0.x",
  "main": "src/server.js",
  "scripts": {
    "start": "node_modules/.bin/oak .",
    "dev": "env $(cat .env | grep -v '#' | xargs) npm start",
    "rebuild": "rm -rf node_modules/ package-lock.json && npm i && node_modules/.bin/oak-rebuild $(pwd)",
    "test": "exit 0;"
  },
  "engines": {
    "node": "10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OakLabsInc/app-lights.git"
  },
