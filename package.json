  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/OakLabsInc/app-lights/issues"
  },
  "homepage": "https://github.com/OakLabsInc/app-lights#readme",
  "devDependencies": {
    "oak": "~5.0.9"
  },
  "dependencies": {
    "@oaklabs/platform": "~3.0.1",
    "angular": "~1.7.2",
    "angular-animate": "~1.7.2",
    "angular-aria": "~1.7.5",
    "angular-material": "~1.1.10",
    "angular-messages": "~1.7.5",
    "express": "~4.16.3",
    "gulp": "^3.9.1",
    "oak-tools": "~0.5.7",
